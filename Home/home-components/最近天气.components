      "updateAt": "2024-06-12T11:10:52.003Z",
      "query": "const key = \"8c72ea310a1a4ae3b59e9f76b20c6698\";  // 在和风天气中创建的 Api key，网址为：<https://dev.qweather.com/docs/configuration/project-and-key/>，请改为你自己的api-key\n\nlet city = '{{city}}';  // 城市名 为空自动定位\nlet days = '{{days}}';  // 天气预报天数1-7\nlet anotherCity = '{{anotherCity}}';  // 添加另外一个城市\nlet locationId = '';\n\nweatherView();\n\n// 主函数\nasync function weatherView() {\n    // 位置信息\n    if (city) {\n        let location = await searchCity(city);\n        locationId = location.id;\n    } else {\n        let location = await getpos();\n        locationId = location.id;\n        city = location.name\n    }\n\n    let weather = await getWeather(locationId, days);\n    let today = weather[0];\n\n    // 空气质量\n    let air = await getair(locationId);\n    let air_category = '';\n    if (air.code == 200) air_category = air.now.category;\n    // 风速\n    let windyspeed = Math.max(today.windSpeedDay, today.windSpeedNight)\n    // 风速描述\n    let windydesc = '';\n    if (windyspeed < 12) {\n        windydesc = \"微风习习\";\n    } else if (windyspeed < 39) {\n        windydesc = \"清风徐徐\";\n    } else {\n        windydesc = \"有\" + today.windDirDay + \"风出没，风力\" + today.windScaleDay + '级';\n    }\n\n    // dv.paragraph(`日升：${today.sunrise}，日落：${today.sunset}`);\n    // dv.paragraph(`月升：${today.moonrise}，月落：${today.moonset}`);\n\n    //输出天气相关内容\n    let current = getCurrentDate();\n    let greeting = getGreeting(current.hours);\n    //today.date = `${current.year}-${current.month}-${current.day}`;\n    today.date = moment().format(\"YYYY-MM-DD\");\n    let moonIndex = moment().diff(moment().startOf('year'), \"hours\").toString().padStart(4, '0');\n    let moonImage = `<img style=\"margin-top:-50px;vertical-align: bottom; -webkit-clip-path: circle(42.55% at 50% 50%);\" width=\"50\" alt=\"|inl\" src=\"https://svs.gsfc.nasa.gov/vis/a000000/a004900/a004955/frames/216x216_1x1_30p/moon.${moonIndex}.jpg\">`\n    let desc = greeting + `今天是${current.year}年${current.month}月${current.day}日 ${current.week}` + `，${city} ${today.textDay}，${today.tempMin}~${today.tempMax}℃，==${air_category}==，${windydesc}，云朵充盈了${today.cloud}%的天空。\\n顺便，月亮会在${today.moonrise} 时浮起，${today.moonset} 时沉落\\n 如果足够幸运碰见它的话，我想它应该是这样的👉🏻`;\n\n    // 添加Header\n    dv.header(3, \"最近天气\");\n\n    // 生成天气动画\n    weatherIcon(weather[0].iconDay, windyspeed)\n\n    //生成最近天气（表格视图）\n    dv.paragraph(`${city} 最近 ${days} 天的天气如下，参考天气制定你的计划吧！`);\n    dv.table([\"日期\", \"天气\", \"温度\", \"云量\", \"月相\"], weather.map(t => [t.fxDate.substring(5), t.textDay, t.tempMin + \"~\" + t.tempMax + \"℃\", t.cloud + \"%\", t.moonPhase]));\n\n    // 生成今日天气\n    dv.paragraph(desc + moonImage);\n\n    // 添加另一个城市\n    if (anotherCity != \"\") {\n        let location = await searchCity(anotherCity);\n        let anotherlocationId = location.id;\n        let anotherWeather = await getWeather(anotherlocationId, 1);\n        let cares = `(对了，还有你关心的${anotherCity},今天的天气是${anotherWeather[0].textDay.replace(/[\\u4e00-\\u9fa5]/g, \"\")}，${anotherWeather[0].tempMin}~${anotherWeather[0].tempMax}℃)`;\n        dv.el(\"blockquote\", cares);\n    }\n}\n\n// 生成问候语\nfunction getGreeting(hour) {\n    if (hour < 6) {\n        return \"<span style='color: var(--text-accent);font-size: 24px;'>凌</span>晨好，\";\n    } else if (hour < 8) {\n        return \"<span style='color: var(--text-accent);font-size: 24px;'>早</span>上好，\";\n    } else if (hour < 12) {\n        return \"<span style='color: var(--text-accent);font-size: 24px;'>上</span>午好，\";\n    } else if (hour < 13) {\n        return \"<span style='color: var(--text-accent);font-size: 24px;'>中</span>午好，\";\n    } else if (hour < 18) {\n        return \"<span style='color: var(--text-accent);font-size: 24px;'>下</span>午好，\";\n    } else {\n        return \"<span style='color: var(--text-accent);font-size: 24px;'>晚</span>上好，\";\n    }\n}\n\n// 生成日期和星期\nfunction getCurrentDate() {\n    const now = new Date();\n    const year = now.getFullYear();\n    const month = String(now.getMonth() + 1).padStart(2, \"0\"); // 月份是从0开始的\n    const day = now.getDate();\n    const week = '星期' + '日一二三四五六'.charAt(now.getDay()); //获取当前星期几(0 ~ 6,0代表星期天)\n    const hours = now.getHours();\n    return {year, month, day, week, hours};\n}\n\n// 获取天气信息\nasync function getWeather(locationId, days) {\n\n    console.log('locationId:' + locationId);\n    let weatherUrl = `\nhttps://devapi.qweather.com/v7/weather/7d?location=${locationId}&key=${key}`;\n    let wUrl = new URL(weatherUrl);\nconst res = await request({\n    url: wUrl.href,\n    method: \"GET\",\n});\n\nlet data = JSON.parse(res);\nif (data.code != \"200\") {\n    return -1;\n}\nlet weather = data.daily.slice(0, days);\n\n// 添加表情\nfor (let i = 0; i < days; i++) {\n    let textDay = weather[i].textDay;\n    let moon = weather[i].moonPhase;\n    if (textDay.includes(\"雨\")) {\n        weather[i].textDay = \"🌧\" + textDay;\n    } else if (textDay.includes(\"云\")) {\n        weather[i].textDay = \"⛅\" + textDay;\n    } else if (textDay.includes(\"晴\")) {\n        weather[i].textDay = \"🌞\" + textDay;\n    } else if (textDay.includes(\"雪\")) {\n        weather[i].textDay = \"❄\" + textDay;\n    } else if (textDay.includes(\"阴\")) {\n        weather[i].textDay = \"🌥\" + textDay;\n    } else if (textDay.includes(\"风\")) {\n        weather[i].textDay = \"🍃\" + textDay;\n    } else if (textDay.includes(\"雷\")) {\n        weather[i].textDay = \"⛈\" + textDay;\n    } else if (textDay.includes(\"雾\")) {\n        weather[i].textDay = \"🌫\" + textDay;\n    }\n    switch (moon) {\n        case \"新月\":\n            weather[i].moonPhase = \"🌑\" + moon;\n            break;\n        case \"峨眉月\":\n            weather[i].moonPhase = \"🌒\" + moon;\n            break;\n        case \"朔月\":\n            weather[i].moonPhase = \"🌑\" + moon;\n            break;\n        case \"娥眉月\":\n            weather[i].moonPhase = \"🌒\" + moon;\n            break;\n        case \"上弦月\":\n            weather[i].moonPhase = \"🌓\" + moon;\n            break;\n        case \"盈凸月\":\n            weather[i].moonPhase = \"🌔\" + moon;\n            break;\n        case \"满月\":\n            weather[i].moonPhase = \"🌕\" + moon;\n            break;\n        case \"亏凸月\":\n            weather[i].moonPhase = \"🌖\" + moon;\n            break;\n        case \"下弦月\":\n            weather[i].moonPhase = \"🌗\" + moon;\n            break;\n        default:\n            weather[i].moonPhase = \"🌘\" + moon;\n    }\n}\nreturn weather;\n}\n\n// 获取空气质量信息\nasync function getair(locationId) {\n\n    let weatherUrl = `https://devapi.qweather.com/v7/air/now?location=${locationId}&key=${key}`;\n    let wUrl = new URL(weatherUrl);\n    const res = await request({\n        url: wUrl.href, method: \"GET\",\n    });\n\n    let data = JSON.parse(res);\n    if (data.code != \"200\") {\n        return -1;\n    }\n    let air = data;\n    return air;\n\n}\n\n// 查询位置\nasync function urlGet(url) {\n    console.log(url);\n    let finalURL = new URL(url);\n    const res = await request({\n        url: finalURL.href,\n        method: \"GET\",\n        cache: \"no-cache\",\n        headers: {\n            'Content-Type': 'application/json;charset=gb2312',\n            'User-Agent': 'Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/98.100.4758.11 Safari/537.36'\n        },\n    });\n\n    return res;\n\n}\n\nasync function getpos() {\n    let result = await urlGet('http://whois.pconline.com.cn/ipJson.jsp?json=true')\n    result = JSON.parse(result);\n    let city = result.cityCode;\n    return await searchCity(city);\n}\n\n// 查询城市ID\nasync function searchCity(city) {\n    let searchUrl = `https://geoapi.qweather.com/v2/city/lookup?location=${city}&key=${key}&number=1`;\n    let sUrl = new URL(searchUrl);\n    const res = await request({\n        url: sUrl.href,\n        method: \"GET\",\n    });\n    let data = JSON.parse(res);\n    if (data.code == \"200\") {\n        return data.location[0];\n    }\n    return -1;\n}\n\n// 生成天气动画\nfunction weatherIcon(iconDay, windySpeed) {\n\n    let weatherNumber = Number(iconDay);\n    console.log(\"windySpeed:\" + windySpeed);\n    let sunny_wind_arr = [];\n    let windy_arr = [];\n    if (windySpeed > 39) {  // 6级大风\n        sunny_wind_arr = [100, 102, 150, 152];\n        windy_arr = [103, 104, 151, 153, 154, 507, 508];\n    }\n    let sun_arr = [100];\n    let cloudy_sun_arr = [103];\n    let cloudy_lightning_arr = [302, 304, 350, 351];\n    let cloud_arr = [101, 102, 104, 154];\n    let rain_arr = [300, 305, 306, 307, 308, 309, 310, 311, 312, 313, 314, 315, 316, 317, 318, 399];\n    let cloudy_rain_lightning_arr = [301, 303];\n    let fog_arr = [500, 501, 502, 503, 504, 509, 510, 511, 512, 513, 514, 515];\n    let snow_arr = [400, 401, 402, 403, 404, 405, 406, 407, 408, 409, 410, 456, 457, 499];\n    let moon_arr = [150];\n    let cloudy_moon_arr = [151, 152, 153];\n\n    if (sun_arr.includes(weatherNumber)) {\n        dv.paragraph(sun)\n    }\n    if (cloudy_sun_arr.includes(weatherNumber)) {\n        dv.paragraph(cloudy_sun)\n    }\n    if (cloud_arr.includes(weatherNumber)) {\n        dv.paragraph(cloud)\n    }\n    if (rain_arr.includes(weatherNumber)) {\n        dv.paragraph(rain)\n    }\n    if (cloudy_lightning_arr.includes(weatherNumber)) {\n        dv.paragraph(cloudy_lightning)\n    }\n    if (cloudy_rain_lightning_arr.includes(weatherNumber)) {\n        dv.paragraph(cloudy_rain_lightning)\n    }\n    if (windy_arr.includes(weatherNumber)) {\n        dv.paragraph(windy)\n    }\n    if (fog_arr.includes(weatherNumber)) {\n        dv.paragraph(fog)\n    }\n    if (snow_arr.includes(weatherNumber)) {\n        dv.paragraph(snow)\n    }\n    if (moon_arr.includes(weatherNumber)) {\n        dv.paragraph(moon)\n    }\n    if (cloudy_moon_arr.includes(weatherNumber)) {\n        dv.paragraph(cloudy_moon)\n    }\n    if (sunny_wind_arr.includes(weatherNumber)) {\n        dv.paragraph(sunny_wind)\n    }\n}\n\nlet sun = `<div class=\"svg-contain\"> <svg version=\"1.1\" id=\"Layer_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\" viewBox=\"0 0 44.9 44.9\" style=\"enable-background:new 0 0 44.9 44.9;\" xml:space=\"preserve\" height=\"40px\" width=\"40px\">  <g id=\"Sun\">    <circle id=\"XMLID_61_\" class=\"yellow\" cx=\"22.4\" cy=\"22.6\" r=\"11\"/>  <g>      <path id=\"XMLID_60_\" class=\"yellow\" d=\"M22.6,8.1h-0.3c-0.3,0-0.6-0.3-0.6-0.6v-7c0-0.3,0.3-0.6,0.6-0.6l0.3,0c0.3,0,0.6,0.3,0.6,0.6 v7C23.2,7.8,22.9,8.1,22.6,8.1z\"/>      <path id=\"XMLID_59_\" class=\"yellow\" d=\"M22.6,36.8h-0.3c-0.3,0-0.6,0.3-0.6,0.6v7c0,0.3,0.3,0.6,0.6,0.6h0.3c0.3,0,0.6-0.3,0.6-0.6v-7 C23.2,37,22.9,36.8,22.6,36.8z\"/>      <path id=\"XMLID_58_\" class=\"yellow\" d=\"M8.1,22.3v0.3c0,0.3-0.3,0.6-0.6,0.6h-7c-0.3,0-0.6-0.3-0.6-0.6l0-0.3c0-0.3,0.3-0.6,0.6-0.6h7 C7.8,21.7,8.1,21.9,8.1,22.3z\"/>      <path id=\"XMLID_57_\" class=\"yellow\" d=\"M36.8,22.3v0.3c0,0.3,0.3,0.6,0.6,0.6h7c0.3,0,0.6-0.3,0.6-0.6v-0.3c0-0.3-0.3-0.6-0.6-0.6h-7 C37,21.7,36.8,21.9,36.8,22.3z\"/>      <path id=\"XMLID_56_\" class=\"yellow\" d=\"M11.4,31.6l0.2,0.3c0.2,0.2,0.2,0.6-0.1,0.8l-5.3,4.5c-0.2,0.2-0.6,0.2-0.8-0.1l-0.2-0.3 c-0.2-0.2-0.2-0.6,0.1-0.8l5.3-4.5C10.9,31.4,11.2,31.4,11.4,31.6z\"/>      <path id=\"XMLID_55_\" class=\"yellow\" d=\"M33.2,13l0.2,0.3c0.2,0.2,0.6,0.3,0.8,0.1l5.3-4.5c0.2-0.2,0.3-0.6,0.1-0.8l-0.2-0.3 c-0.2-0.2-0.6-0.3-0.8-0.1l-5.3,4.5C33,12.4,33,12.7,33.2,13z\"/>      <path id=\"XMLID_54_\" class=\"yellow\" d=\"M11.4,13.2l0.2-0.3c0.2-0.2,0.2-0.6-0.1-0.8L6.3,7.6C6.1,7.4,5.7,7.5,5.5,7.7L5.3,7.9 C5.1,8.2,5.1,8.5,5.4,8.7l5.3,4.5C10.9,13.5,11.2,13.5,11.4,13.2z\"/>      <path id=\"XMLID_53_\" class=\"yellow\" d=\"M33.2,31.9l0.2-0.3c0.2-0.2,0.6-0.3,0.8-0.1l5.3,4.5c0.2,0.2,0.3,0.6,0.1,0.8l-0.2,0.3 c-0.2,0.2-0.6,0.3-0.8,0.1l-5.3-4.5C33,32.5,33,32.1,33.2,31.9z\"/>    <animate attributeType=\"CSS\"      attributeName=\"opacity\"      attributeType=\"XML\"      dur=\"0.5s\"      keyTimes=\"0;0.5