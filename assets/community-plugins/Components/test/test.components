      "markdownValue": "```dataviewjs\nweatherView()\n\n// 主函数\nasync function weatherView() {\n    let current = getCurrentDate();\n    let history = Object.assign(JSON.parse(await app.vault.adapter.read(\"assets/community-plugins/Dataview/data/daily-stats.json\")));\n    // let today = moment().format(\"YYYY-MM-DD\");\n    let today = `${current.year}-${current.month}-${current.day}`;\n\n    if (history.hasOwnProperty(today)) {\n        let weather = history[today].weather;\n        let iconDay = weather[0].iconDay;\n        let windSpeedDay = weather[0].windSpeedDay;\n        let windSpeedNight = weather[0].windSpeedNight;\n        if (iconDay) {\n            // 输出天气动画\n            weatherIcon(iconDay, Math.max(windSpeedDay, windSpeedNight))\n        }\n    }\n}\n\n// 生成日期和星期\nfunction getCurrentDate() {\n    const now = new Date();\n    const year = now.getFullYear();\n    const month = String(now.getMonth() + 1).padStart(2, \"0\"); // 月份是从0开始的\n    const day = String(now.getDate()).padStart(2, \"0\");\n    const week = '星期' + '日一二三四五六'.charAt(now.getDay()); //获取当前星期几(0 ~ 6,0代表星期天)\n    const hours = now.getHours();\n    return {year, month, day, week, hours};\n}\n\n// 生成天气动画\nfunction weatherIcon(iconDay, windyspeed) {\n    let weatherNumber = Number(iconDay);\n    let sunny_wind_arr = [];\n    let windy_arr = [];\n\n    if (windyspeed > 39) {  // 6级大风\n        sunny_wind_arr = [100, 102, 150, 152];\n        windy_arr = [103, 104, 151, 153, 154, 507, 508];\n    }\n    let sun_arr = [100];\n    let cloudy_sun_arr = [103];\n    let cloudy_lightning_arr = [302, 304, 350, 351];\n    let cloud_arr = [101, 102, 104, 154];\n    let rain_arr = [300, 305, 306, 307, 308, 309, 310, 311, 312, 313, 314, 315, 316, 317, 318, 399];\n    let cloudy_rain_lightning_arr = [301, 303];\n    let fog_arr = [500, 501, 502, 503, 504, 509, 510, 511, 512, 513, 514, 515];\n    let snow_arr = [400, 401, 402, 403, 404, 405, 406, 407, 408, 409, 410, 456, 457, 499];\n    let moon_arr = [150];\n    let cloudy_moon_arr = [151, 152, 153];\n    //weather[0].windScale 风力\n\n    if (sun_arr.includes(weatherNumber)) {\n        dv.paragraph(sun)\n    }\n    if (cloudy_sun_arr.includes(weatherNumber)) {\n        dv.paragraph(cloudy_sun)\n    }\n    if (cloud_arr.includes(weatherNumber)) {\n        dv.paragraph(cloud)\n    }\n    if (rain_arr.includes(weatherNumber)) {\n        dv.paragraph(rain)\n    }\n    if (cloudy_lightning_arr.includes(weatherNumber)) {\n        dv.paragraph(cloudy_lightning)\n    }\n    if (cloudy_rain_lightning_arr.includes(weatherNumber)) {\n        dv.paragraph(cloudy_rain_lightning)\n    }\n    if (windy_arr.includes(weatherNumber)) {\n        dv.paragraph(windy)\n    }\n    if (fog_arr.includes(weatherNumber)) {\n        dv.paragraph(fog)\n    }\n    if (snow_arr.includes(weatherNumber)) {\n        dv.paragraph(snow)\n    }\n    if (moon_arr.includes(weatherNumber)) {\n        dv.paragraph(moon)\n    }\n    if (cloudy_moon_arr.includes(weatherNumber)) {\n        dv.paragraph(cloudy_moon)\n    }\n    if (sunny_wind_arr.includes(weatherNumber)) {\n        dv.paragraph(sunny_wind)\n    }\n}\n\nlet sun = `<div class=\"svg-contain\"> <svg version=\"1.1\" id=\"Layer_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\" viewBox=\"0 0 44.9 44.9\" style=\"enable-background:new 0 0 44.9 44.9;\" xml:space=\"preserve\" height=\"40px\" width=\"40px\">  <g id=\"Sun\">    <circle id=\"XMLID_61_\" class=\"yellow\" cx=\"22.4\" cy=\"22.6\" r=\"11\"/>  <g>      <path id=\"XMLID_60_\" class=\"yellow\" d=\"M22.6,8.1h-0.3c-0.3,0-0.6-0.3-0.6-0.6v-7c0-0.3,0.3-0.6,0.6-0.6l0.3,0c0.3,0,0.6,0.3,0.6,0.6 v7C23.2,7.8,22.9,8.1,22.6,8.1z\"/>      <path id=\"XMLID_59_\" class=\"yellow\" d=\"M22.6,36.8h-0.3c-0.3,0-0.6,0.3-0.6,0.6v7c0,0.3,0.3,0.6,0.6,0.6h0.3c0.3,0,0.6-0.3,0.6-0.6v-7 C23.2,37,22.9,36.8,22.6,36.8z\"/>      <path id=\"XMLID_58_\" class=\"yellow\" d=\"M8.1,22.3v0.3c0,0.3-0.3,0.6-0.6,0.6h-7c-0.3,0-0.6-0.3-0.6-0.6l0-0.3c0-0.3,0.3-0.6,0.6-0.6h7 C7.8,21.7,8.1,21.9,8.1,22.3z\"/>      <path id=\"XMLID_57_\" class=\"yellow\" d=\"M36.8,22.3v0.3c0,0.3,0.3,0.6,0.6,0.6h7c0.3,0,0.6-0.3,0.6-0.6v-0.3c0-0.3-0.3-0.6-0.6-0.6h-7 C37,21.7,36.8,21.9,36.8,22.3z\"/>      <path id=\"XMLID_56_\" class=\"yellow\" d=\"M11.4,31.6l0.2,0.3c0.2,0.2,0.2,0.6-0.1,0.8l-5.3,4.5c-0.2,0.2-0.6,0.2-0.8-0.1l-0.2-0.3 c-0.2-0.2-0.2-0.6,0.1-0.8l5.3-4.5C10.9,31.4,11.2,31.4,11.4,31.6z\"/>      <path id=\"XMLID_55_\" class=\"yellow\" d=\"M33.2,13l0.2,0.3c0.2,0.2,0.6,0.3,0.8,0.1l5.3-4.5c0.2-0.2,0.3-0.6,0.1-0.8l-0.2-0.3 c-0.2-0.2-0.6-0.3-0.8-0.1l-5.3,4.5C33,12.4,33,12.7,33.2,13z\"/>      <path id=\"XMLID_54_\" class=\"yellow\" d=\"M11.4,13.2l0.2-0.3c0.2-0.2,0.2-0.6-0.1-0.8L6.3,7.6C6.1,7.4,5.7,7.5,5.5,7.7L5.3,7.9 C5.1,8.2,5.1,8.5,5.4,8.7l5.3,4.5C10.9,13.5,11.2,13.5,11.4,13.2z\"/>      <path id=\"XMLID_53_\" class=\"yellow\" d=\"M33.2,31.9l0.2-0.3c0.2-0.2,0.6-0.3,0.8-0.1l5.3,4.5c0.2,0.2,0.3,0.6,0.1,0.8l-0.2,0.3 c-0.2,0.2-0.6,0.3-0.8,0.1l-5.3-4.5C33,32.5,33,32.1,33.2,31.9z\"/>    <animate attributeType=\"CSS\"      attributeName=\"opacity\"      attributeType=\"XML\"      dur=\"0.5s\"      keyTimes=\"0;0.5;1\"      repeatCount=\"indefinite\"      values=\"1;0.6;1\"      calcMode=\"linear\"/>  </g></g>  </svg></div>`;\nlet moon = `<div class=\"svg-contain\"><svg version=\"1.1\" id=\"Layer_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\" viewBox=\"0 0 30.8 42.5\" style=\"enable-background:new 0 0 30.8 42.5;\" xml:space=\"preserve\" height=\"40px\" width=\"40px\">  <path id=\"Moon\" class=\"yellow\" d=\"M15.3,21.4C15,12.1,21.1,4.2,29.7,1.7c-2.8-1.2-5.8-1.8-9.1-1.7C8.9,0.4-0.3,10.1,0,21.9 c0.3,11.7,10.1,20.9,21.9,20.6c3.2-0.1,6.3-0.9,8.9-2.3C22.2,38.3,15.6,30.7,15.3,21.4z\"/>  </svg></div>`;\nlet rain = `  <div class=\"svg-contain\">  <svg version=\"1.1\" id=\"Layer_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0