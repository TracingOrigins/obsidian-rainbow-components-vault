      "query": "let month = moment().format('MM')\nlet date = moment().format('MMDD')\nlet url = `https://baike.baidu.com/cms/home/eventsOnHistory/${month}.json`\nlet res = await request({url: url,method: \"GET\"});\nlet data = JSON.parse(res)\nconsole.log(data[month][date])\ndv.table(\n\t['年份','事件','详细内容'],\n\tdata[month][date].map(p=>[p.year,p.title,`[详情](${p.link})`])\n\t)",
      "queryType": "dataviewjs",
      "maxHeight": 300,
      "dynamicParamComponents": [],
